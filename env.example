# Copy this file to .env and update values as needed

# Google GenAI (AI Studio) key for Gemini models
GOOGLE_API_KEY=ai-studio-sample-key

# Use Vertex AI instead of AI Studio (TRUE|FALSE)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# If using Vertex AI, set these (sample project/region)
GOOGLE_CLOUD_PROJECT=my-sample-project
GOOGLE_CLOUD_LOCATION=us-central1

# BigQuery (optional for schema introspection and running tests)
BIGQUERY_PROJECT_ID=my-sample-project
BIGQUERY_DATASET_RAW=raw
BIGQUERY_DATASET_STAGING=temp
BIGQUERY_DATASET_FINAL=analytics

# BigQuery Emulator (optional). Example: http://127.0.0.1:9050
BIGQUERY_EMULATOR_HOST=http://127.0.0.1:9050

# CLI defaults
ADK_DEFAULT_MODEL=gemini-2.0-flash
